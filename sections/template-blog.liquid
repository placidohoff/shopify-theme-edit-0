{% paginate blog.articles by 3 %}
    {% for product in blog.articles %}
      <div>
        {% if article.image %}
            <img src="{{article.image | img_url: 'medium'}}"  alt="{{article.image.alt}}" />
        {% else %}
            {{'image' |  placeholder_svg_tag}}
        {% endif %}
      </div>
    {% endfor %}
{% endpaginate %}
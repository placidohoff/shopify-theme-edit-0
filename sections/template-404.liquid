<h1>{{section.settings.title}}</h1>
<p>{{section.settings.subtext_title}}</p>

{% schema %}
{
    "name": "Template 404",
    "settings": [
        {
            "type": "text",
            "id": "title",
            "default": "Page Not Found",
            "label": "404 Heading Title"
        },
        {
            "type": "text",
            "id": "subtext_title",
            "default": "The page you are requesting cannot be found",
            "label": "404 Subtext title"
        }
        ]
}
{% endschema %}